<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="500dp"
    android:layout_gravity="bottom"
    tools:context="com.example.yogesh.life.Review_Fragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="20dp"
        android:layout_gravity="center">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Doctors I contacted"
            android:id="@+id/textView13"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Dr. Khanna"
            android:id="@+id/textView14"
            android:layout_below="@+id/textView13"
            android:layout_alignParentStart="true"
            android:layout_marginTop="42dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Dr. Batra"
            android:id="@+id/textView15"
            android:layout_centerVertical="true"
            android:layout_alignStart="@+id/textView14" />

        <RatingBar
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:id="@+id/ratingBar"
            android:layout_below="@+id/textView14"
            android:layout_alignStart="@+id/textView14"
            android:rating="4.0"
            android:layout_alignEnd="@+id/ratingBar2" />

        <RatingBar
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:id="@+id/ratingBar2"
            android:layout_below="@+id/textView15"
            android:layout_alignStart="@+id/textView15"
            android:rating="2.5"/>

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="RATE"
            android:id="@+id/button5"
            android:layout_alignTop="@+id/ratingBar"
            android:layout_toEndOf="@+id/ratingBar" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="RATE"
            android:id="@+id/button6"
            android:layout_alignTop="@+id/ratingBar2"
            android:layout_alignEnd="@+id/button5"
            android:layout_toEndOf="@+id/ratingBar" />

    </RelativeLayout>

</FrameLayout>
